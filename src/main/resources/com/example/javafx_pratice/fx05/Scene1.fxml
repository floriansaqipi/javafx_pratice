      <Label layoutX="114.0" layoutY="180.0" text="Username: ">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="225.0" layoutY="177.0" promptText="Enter Username here " />
      <Button fx:id="loginButton" layoutX="274.0" layoutY="299.0" mnemonicParsing="false" onAction="#login" text="Log in" />
